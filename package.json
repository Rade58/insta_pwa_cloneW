{
  "name": "insta_pwa_clone_w",
  "version": "1.0.1",
  "description": "app made for practicing webpack and pwa techologies",
  "main": "index.js",
  "repository": "https://github.com/Rade58/insta_pwa_cloneW.git",
  "author": "Rade",
  "license": "MIT",
  "private": false,
  "scripts": {
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "dev": "npm run webpack-dev-server -- --env.mode development --hot",
    "prod": "npm run webpack -- --env.mode production",
    "dev:debug": "npm run debug -- --env.mode development",
    "prod:debug": "npm run debug -- --env.mode production",
    "prod:compress": "npm run prod -- --env.presets compress",
    "prod:single_compress_serve": "npm run prod -- --env.presets single_compress_serve",
    "prod:multi_compress_serve": "npm run prod -- --env.presets multi_compress_serve",
    "prod:multi": "npm run prod -- --env.presets multi",
    "dev:multi": "npm run dev -- --env.presets multi",
    "dev:serviceworker": "npm run dev -- --env presets serviceworker",
    "prod:serviceworker": "npm run prod -- --env presets serviceworker",
    "dev:multi:serviceworker": "npm run dev:multi -- --env.presets serviceworker",
    "prod:multi:serviceworker": "npm run prod:multi -- --env.presets serviceworker",
    "prod:single_compress_serve:serviceworker": "npm run prod:single_compress_serve -- --env.presets serviceworker",
    "prod:multi_compress_serve:serviceworker": "npm run prod:multi_compress_serve -- --env.presets serviceworker",
    "dev:multi:serviceworker:pwamanifest": "npm run dev:multi:serviceworker -- --env.presets pwamanifest",
    "dev:multi:serviceworker:pwamanifest:copy": "npm run dev:multi:serviceworker:pwamanifest -- --env.presets copy",
    "prod:single_compress_serve:serviceworker:pwamanifest": "npm run prod:single_compress_serve:serviceworker -- --env.presets pwamanifest",
    "prod:multi_compress_serve:serviceworker:pwamanifest": "npm run prod:multi_compress_serve:serviceworker -- --env.presets pwamanifest",
    "prod:single_compress_serve:serviceworker:pwamanifest:copy": "npm run prod:single_compress_serve:serviceworker:pwamanifest -- --env.presets copy",
    "prod:multi_compress_serve:serviceworker:pwamanifest:copy": "npm run prod:multi_compress_serve:serviceworker:pwamanifest -- --env.presets copy",
    "dev:multi:copy": "npm run dev:multi -- --env.presets copy",
    "dev:my-loader:debug": "npm run dev:debug -- --env.presets my-loader",
    "dev:my-loader": "npm run dev -- --env.presets my-loader",
    "prod:my-loader": "npm run prod -- --env.presets my-loader",
    "prod:my-loader:debug": "npm run prod:debug -- --env.presets my-loader",
    "dev:debug:multi": "npm run dev:debug -- --env.presets multi",
    "dev:multi:inject-manifest": "npm run dev:multi -- --env.presets inject_manifest",
    "dev:multi:inject-manifest:sw-libs": "npm run dev:multi:inject-manifest -- --env.presets sw_libs",
    "dev:debug:multi:inject-manifest": "npm run dev:debug:multi -- --env.presets inject_manifest",
    "dev:debug:multi:inject-manifest:sw-libs": "npm run dev:debug:multi:inject-manifest -- --env.presets sw_libs",
    "dev-my-plug": "npm run dev:multi:inject-manifest:sw-libs",
    "dev-debug-my-plug": "npm run dev:debug:multi:inject-manifest:sw-libs",
    "dev:multi:inject-manifest:sw-libs:pwamanifest": "npm run dev:multi:inject-manifest:sw-libs -- --env.presets pwamanifest",
    "prod:multi_compress_serve:inject_manifest": "npm run prod:multi_compress_serve -- --env.presets inject_manifest",
    "prod:multi_compress_serve:inject_manifest:sw_libs": "npm run prod:multi_compress_serve:inject_manifest -- --env.presets sw_libs",
    "prod:multi_compress_serve:inject_manifest:sw_libs:pwamanifest": "npm run prod:multi_compress_serve:inject_manifest:sw_libs -- --env.presets pwamanifest",
    "dev-final": "npm run dev:multi:inject-manifest:sw-libs:pwamanifest",
    "prod-final": "npm run prod:multi_compress_serve:inject_manifest:sw_libs:pwamanifest",
    "start-dist": "http-server ./dist -p 5980 -a localhost -c-1 --gzip",
    "start-blah": "rimraf ./dist && npm run prod-final && npm run start-dist"
  },
  "devDependencies": {
    "compression-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "html-webpack-change-assets-extension-plugin": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "serviceworker-webpack-plugin": "^1.0.1",
    "style-loader": "^1.0.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.3",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2",
    "webpack-pwa-manifest": "^4.0.0",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "dependencies": {
    "normalize-path": "^3.0.0"
  }
}
